{
  "name": "deno.ns",
  "version": "0.1.1",
  "description": "Deno namespace shim for Node.js",
  "keywords": [
    "deno namespace",
    "deno",
    "polyfill",
    "ponyfill",
    "shim"
  ],
  "main": "./dist/index.js",
  "author": "Thomas Rory Gummerson <thomas@gummerson.no> (https://trgwii.no/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trgwii/deno.ns.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./global": "./dist/global.js",
    "./deno": "./dist/deno.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "denolib": "deno run tools/deno_check.ts && deno types > src/lib.deno.d.ts",
    "build": "tsc",
    "prepare": "npm run --silent denolib && npm run --silent build",
    "lint": "deno lint --ignore=node_modules,dist,deno,unit,src/lib.deno.d.ts --unstable",
    "fmt": "deno fmt --ignore=node_modules,dist,deno,unit,src/lib.deno.d.ts",
    "clean": "git clean -fXde '!node_modules/'",
    "deno-test": "node dist/cli/test.js",
    "deno-unit-tests": "npm run --silent build & deno run -A --unstable tools/run_deno_unit_tests.ts"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
