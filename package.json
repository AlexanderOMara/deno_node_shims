{
	"name": "deno.ns",
	"version": "0.0.1",
	"description": "Deno namespace shim for node",
	"main": "./dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./global": "./dist/global.js"
	},
	"scripts": {
		"denolib": "deno types > src/lib.deno.d.ts",
		"build": "tsc",
		"bundle": "deno run --allow-read=dist --allow-write=deno.ns.b.ts https://raw.githubusercontent.com/trgwii/bundler/master/bundler.ts ts-bundle dist deno.ns.b.ts",
		"full": "npm run denolib & npm run build & npm run bundle"
	},
	"devDependencies": {
		"@types/node": "^15.0.1",
		"@types/node-fetch": "^2.5.10"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
